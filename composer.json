{
    "name": "bolt/socialite",
    "description": "A combined social media widget with lazy-loading capabilities",
    "type": "bolt-extension",
    "keywords": [
        "Social Media",
        "Facebook",
        "Google+",
        "Twitter",
        "GitHub"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\Socialite\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Bolt\\Socialite\\SocialiteExtension",
        "bolt-icon": "icon.png",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
